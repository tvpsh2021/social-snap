{
  "name": "social-snap",
  "version": "2.0.0",
  "description": "Social Snap - A Chrome extension that allows you to download all images from Threads.com and Instagram.com posts with a single click",
  "scripts": {
    "lint": "npm run lint:js && npm run lint:html",
    "lint:js": "eslint *.js",
    "lint:html": "htmlhint *.html",
    "lint:fix": "npm run lint:js:fix && npm run format:html",
    "lint:js:fix": "eslint *.js --fix",
    "format:html": "prettier --write *.html"
  },
  "devDependencies": {
    "eslint": "^8.57.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-promise": "^6.1.1",
    "htmlhint": "^1.1.4",
    "prettier": "^3.2.5"
  },
  "keywords": [
    "chrome-extension",
    "social-media",
    "image-downloader",
    "batch-download",
    "threads",
    "instagram",
    "social-snap"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": ""
  }
}
